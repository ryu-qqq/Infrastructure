{
  "_module": {
    "name": "Dedupe Check Pattern",
    "description": "중복 여부에 따라 생성/업데이트 분기하는 패턴",
    "version": "1.0.0",
    "inputs": ["exists", "issue_key"],
    "outputs": ["create_new", "update_existing"]
  },
  "nodes": [
    {
      "parameters": {
        "conditions": {
          "options": {"caseSensitive": true, "leftValue": "", "typeValidation": "strict", "version": 2},
          "conditions": [
            {
              "id": "exists-check",
              "leftValue": "={{ $json.exists }}",
              "rightValue": true,
              "operator": {"type": "boolean", "operation": "equals"}
            }
          ],
          "combinator": "and"
        }
      },
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [500, 300],
      "id": "if-exists",
      "name": "IF Exists"
    }
  ],
  "connections": {},
  "_usage": {
    "description": "search-duplicate 모듈 후에 사용",
    "flow": {
      "true_output": "기존 이슈에 코멘트 추가 (add-comment 모듈)",
      "false_output": "새 이슈 생성 (create-issue 모듈)"
    },
    "example_flow": [
      "1. Parse Sentry Data",
      "2. Search Duplicate (fingerprint 기반)",
      "3. IF Exists (이 모듈)",
      "4a. True → Add Comment to existing issue",
      "4b. False → Create New Issue"
    ]
  }
}
