{
  "module": "s3-bucket",
  "version": "1.0.0",
  "description": "S3 bucket with encryption and versioning",
  "category": "storage",
  "parameters": {
    "required": {
      "bucket_purpose": {
        "type": "string",
        "description": "Bucket purpose (e.g., uploads, backups, logs)",
        "default": "data"
      }
    }
  },
  "optional_components": {
    "versioning": {
      "type": "boolean",
      "question": "버전 관리를 활성화하시겠습니까?",
      "default": true
    },
    "lifecycle_enabled": {
      "type": "boolean",
      "question": "수명 주기 정책을 활성화하시겠습니까?",
      "default": true
    },
    "public_access": {
      "type": "boolean",
      "question": "퍼블릭 액세스를 허용하시겠습니까? (일반적으로 false)",
      "default": false
    }
  }
}
